{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "object-curly-spacing": 1,
    "array-bracket-spacing": 1,
    "computed-property-spacing": 1,
    "keyword-spacing": 1,
    "no-empty": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "no-mixed-spaces-and-tabs": 0
  }
}
